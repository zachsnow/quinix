{
  "name": "quinix",
  "version": "0.0.2",
  "description": "A 32-bit RISC virtual machine, assembly language, and C-like high-level language",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Zach Snow",
  "homepage": "https://quinix.vein.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachsnow/quinix.git"
  },
  "bugs": {
    "url": "https://github.com/zachsnow/quinix/issues"
  },
  "keywords": [
    "virtual-machine",
    "compiler",
    "assembler",
    "programming-language",
    "educational"
  ],
  "scripts": {
    "build": "./build.sh",
    "watch": "./watch.sh",
    "test": "bun test",
    "types": "tsc --noEmit",
    "qasm": "bun run bin/qasm.ts",
    "qllc": "bun run bin/qllc.ts",
    "qvm": "bun run bin/qvm.ts",
    "qrun": "bun run bin/qrun.ts",
    "qstrings": "bun run bin/qstrings.ts",
    "qos": "bun run bin/qvm.ts kernel/kernel.qbin --disk image/disk.qfs --display 320x200 --keyboard",
    "bench": "bun run bench/bench.ts",
    "bench:baseline": "bun run bench/baseline.ts",
    "bench:update": "bun run bench/bench.ts --update"
  },
  "dependencies": {
    "peggy": "^4.2.0",
    "ts-pegjs": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.7",
    "bun-types": "^1.3.6",
    "typescript": "^5.7.3"
  }
}
